<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout10"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:ignore="MissingDefaultResource">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="350dp"
        android:layout_height="wrap_content"
        android:text="@string/yasadiginizIlAciklamaTxt"
        android:textSize="11sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ilConstraint"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView35"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:text="@string/baslarkenTxt"
        android:textColor="@color/uygulamaMavisi"
        android:textSize="@dimen/baslarkenBaslikSize400"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.516"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView36"
        android:layout_width="338dp"
        android:layout_height="46dp"
        android:layout_marginTop="8dp"
        android:fontFamily="sans-serif-light"
        android:text="@string/baslarkenMetin"
        android:textAlignment="center"
        android:textSize="13sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView35"
        tools:ignore="MissingConstraints" />


    <TextView
        android:id="@+id/textView37"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:fontFamily="sans-serif-light"
        android:text="@string/yasadiginizIlTxt"
        android:textColor="#000000"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView36" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/ilConstraint"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView37"
        tools:ignore="MissingConstraints">

        <Spinner
            android:id="@+id/ilAutoCompleteTextView"
            android:layout_width="150dp"
            android:layout_height="50dp"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="1dp"
            android:layout_marginBottom="10dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />


    </androidx.constraintlayout.widget.ConstraintLayout>

    <TextView
        android:id="@+id/textView38"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:fontFamily="sans-serif-light"
        android:text="@string/ilgiAlaniTxt"
        android:textColor="#000000"
        android:textSize="18sp"
        android:typeface="normal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ilConstraint" />

    <TextView
        android:id="@+id/textView39"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-light"
        android:text="@string/herKonudaBildirimTxt"
        android:textColor="#000000"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout4"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/yirmiDortSaatBildirimButon"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/baslarkenilbuton"
        android:drawableLeft="@mipmap/bildirim_icon"
        android:textAllCaps="false"
        android:textColor="@color/uygulamaMavisi"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView39"
        tools:ignore="MissingConstraints" />


    <Button
        android:id="@+id/baslarkenSonrakiBtn"
        android:layout_width="170dp"
        android:layout_height="46dp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/backgraoundapp"
        android:text="Sonraki"
        android:textColor="@color/profilSekmeBeyaz"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView9"
        app:layout_constraintVertical_bias="0.365"
        tools:ignore="MissingConstraints" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout4"
        android:layout_width="wrap_content"
        android:layout_height="250dp"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView38">

        <Button
            android:id="@+id/adresEtiketButon"
            android:layout_width="@dimen/baslarkenEtiketButonWidth400"
            android:layout_height="@dimen/baslarkenEtiketButonHeight400"
            android:layout_marginLeft="8dp"
            android:background="@drawable/baslarkenetiketbuton"
            android:drawableLeft="@mipmap/ic_launcher_adres"
            android:paddingLeft="1dp"
            android:text="Adres"
            android:textAllCaps="false"
            android:textColor="@color/uygulamaMavisi"
            android:textSize="@dimen/baslarkenEtiketTextSize400"
            app:layout_constraintStart_toStartOf="parent"
            tools:ignore="MissingConstraints,NotSibling" />

        <Button
            android:id="@+id/yemekEtiketButon"
            android:layout_width="@dimen/baslarkenEtiketButonWidth400"
            android:layout_height="@dimen/baslarkenEtiketButonHeight400"
            android:layout_marginStart="8dp"
            android:background="@drawable/baslarkenetiketbuton"
            android:drawableLeft="@mipmap/yemek_icon"
            android:paddingLeft="1dp"
            android:text="Yemek"
            android:textAllCaps="false"
            android:textColor="@color/uygulamaMavisi"
            android:textSize="@dimen/baslarkenEtiketTextSize400"

            app:layout_constraintStart_toEndOf="@+id/adresEtiketButon"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints,NotSibling" />

        <Button
            android:id="@+id/sporEtiketButon"
            android:layout_width="@dimen/baslarkenEtiketButonWidth400"
            android:layout_height="@dimen/baslarkenEtiketButonHeight400"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="16dp"
            android:layout_marginRight="8dp"
            android:background="@drawable/baslarkenetiketbuton"
            android:drawableLeft="@mipmap/spor_icon"
            android:paddingLeft="1dp"
            android:text="Spor"
            android:textAllCaps="false"
            android:textColor="@color/uygulamaMavisi"
            android:textSize="@dimen/baslarkenEtiketTextSize400"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/yemekEtiketButon"
            app:layout_constraintTop_toBottomOf="@+id/textView38"
            tools:ignore="MissingConstraints,NotSibling" />

        <Button
            android:id="@+id/tatilEtiketButon"
            android:layout_width="@dimen/baslarkenEtiketButonWidth400"
            android:layout_height="@dimen/baslarkenEtiketButonHeight400"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="8dp"
            android:background="@drawable/baslarkenetiketbuton"
            android:drawableLeft="@mipmap/tatil_icon"
            android:paddingLeft="1dp"
            android:text="Tatil"
            android:textAllCaps="false"
            android:textColor="@color/uygulamaMavisi"
            android:textSize="@dimen/baslarkenEtiketTextSize400"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/adresEtiketButon"
            tools:ignore="MissingConstraints" />

        <Button
            android:id="@+id/alisverisEtiketButon"
            android:layout_width="@dimen/baslarkenEtiketButonWidth400"
            android:layout_height="@dimen/baslarkenEtiketButonHeight400"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="8dp"
            android:background="@drawable/baslarkenetiketbuton"
            android:drawableLeft="@mipmap/alisveris_icon"
            android:text="Alışveriş"
            android:textAllCaps="false"
            android:textColor="@color/uygulamaMavisi"
            android:textSize="@dimen/baslarkenEtiketTextSize400"
            app:layout_constraintStart_toEndOf="@+id/tatilEtiketButon"
            app:layout_constraintTop_toBottomOf="@+id/yemekEtiketButon"
            tools:ignore="MissingConstraints" />

        <Button
            android:id="@+id/sanatEtiketButon"
            android:layout_width="@dimen/baslarkenEtiketButonWidth400"
            android:layout_height="@dimen/baslarkenEtiketButonHeight400"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="8dp"
            android:layout_marginRight="8dp"
            android:background="@drawable/baslarkenetiketbuton"
            android:drawableLeft="@drawable/ic_tv_svg"
            android:drawableTint="@color/uygulamaMavisi"
            android:paddingLeft="12dp"
            android:text="Sanat"
            android:textAllCaps="false"
            android:textColor="@color/uygulamaMavisi"
            android:textSize="@dimen/baslarkenEtiketTextSize400"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/alisverisEtiketButon"
            app:layout_constraintTop_toBottomOf="@+id/sporEtiketButon"
            tools:ignore="MissingConstraints" />

        <Button
            android:id="@+id/yazilimEtiketButon"
            android:layout_width="@dimen/baslarkenEtiketButonWidth400"
            android:layout_height="@dimen/baslarkenEtiketButonHeight400"
            android:layout_marginLeft="8dp"
            android:background="@drawable/baslarkenetiketbuton"
            android:drawableLeft="@drawable/ic_engineer"
            android:drawableTint="@color/uygulamaMavisi"
            android:paddingLeft="12dp"
            android:text="Yazılım"
            android:textAllCaps="false"
            android:textColor="@color/uygulamaMavisi"
            android:textSize="@dimen/baslarkenEtiketTextSize400"
            app:layout_constraintStart_toStartOf="parent"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteY="96dp" />

        <Button
            android:id="@+id/otoEtiketButon"
            android:layout_width="@dimen/baslarkenEtiketButonWidth400"
            android:layout_height="@dimen/baslarkenEtiketButonHeight400"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="8dp"
            android:background="@drawable/baslarkenetiketbuton"
            android:drawableLeft="@drawable/ic_auto_svg"
            android:drawableTint="@color/uygulamaMavisi"
            android:paddingLeft="12dp"
            android:text="Oto"
            android:textAllCaps="false"
            android:textColor="@color/uygulamaMavisi"
            android:textSize="@dimen/baslarkenEtiketTextSize400"
            app:layout_constraintStart_toEndOf="@+id/yazilimEtiketButon"
            app:layout_constraintTop_toBottomOf="@+id/alisverisEtiketButon"
            tools:ignore="MissingConstraints" />

        <Button
            android:id="@+id/modaEtiketButon"
            android:layout_width="@dimen/baslarkenEtiketButonWidth400"
            android:layout_height="@dimen/baslarkenEtiketButonHeight400"
            android:layout_marginTop="8dp"
            android:background="@drawable/baslarkenetiketbuton"
            android:drawableLeft="@drawable/ic_moda_svg"
            android:drawableTint="@color/uygulamaMavisi"
            android:paddingLeft="12dp"
            android:text="Moda"
            android:textAllCaps="false"
            android:textColor="@color/uygulamaMavisi"
            android:textSize="@dimen/baslarkenEtiketTextSize400"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/otoEtiketButon"
            app:layout_constraintTop_toBottomOf="@+id/sanatEtiketButon"
            tools:ignore="MissingConstraints" />

        <Button
            android:id="@+id/tarihEtiketButon"
            android:layout_width="@dimen/baslarkenEtiketButonWidth400"
            android:layout_height="@dimen/baslarkenEtiketButonHeight400"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="8dp"
            android:background="@drawable/baslarkenetiketbuton"
            android:drawableLeft="@drawable/ic_moda_svg"
            android:drawableTint="@color/uygulamaMavisi"
            android:paddingLeft="12dp"
            android:text="Tarih"
            android:textAllCaps="false"
            android:textColor="@color/uygulamaMavisi"
            android:textSize="@dimen/baslarkenEtiketTextSize400"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/yazilimEtiketButon"
            tools:ignore="MissingConstraints" />

        <Button
            android:id="@+id/egitimEtiketButon"
            android:layout_width="@dimen/baslarkenEtiketButonWidth400"
            android:layout_height="@dimen/baslarkenEtiketButonHeight400"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="8dp"
            android:background="@drawable/baslarkenetiketbuton"
            android:drawableLeft="@drawable/ic_moda_svg"
            android:drawableTint="@color/uygulamaMavisi"
            android:paddingLeft="12dp"
            android:text="Eğitim"
            android:textAllCaps="false"
            android:textColor="@color/uygulamaMavisi"
            android:textSize="@dimen/baslarkenEtiketTextSize400"
            app:layout_constraintStart_toEndOf="@+id/tarihEtiketButon"
            app:layout_constraintTop_toBottomOf="@+id/otoEtiketButon"
            tools:ignore="MissingConstraints" />

        <Button
            android:id="@+id/muzikEtiketButon"
            android:layout_width="@dimen/baslarkenEtiketButonWidth400"
            android:layout_height="@dimen/baslarkenEtiketButonHeight400"
            android:layout_marginTop="8dp"
            android:background="@drawable/baslarkenetiketbuton"
            android:drawableLeft="@drawable/ic_auto_svg"
            android:drawableTint="@color/uygulamaMavisi"
            android:paddingLeft="12dp"
            android:text="Müzik"
            android:textAllCaps="false"
            android:textColor="@color/uygulamaMavisi"
            android:textSize="@dimen/baslarkenEtiketTextSize400"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/egitimEtiketButon"
            app:layout_constraintTop_toBottomOf="@+id/modaEtiketButon"
            tools:ignore="MissingConstraints" />

        <Button
            android:id="@+id/saglikEtiketButon"
            android:layout_width="@dimen/baslarkenEtiketButonWidth400"
            android:layout_height="@dimen/baslarkenEtiketButonHeight400"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="8dp"
            android:background="@drawable/baslarkenetiketbuton"
            android:drawableLeft="@drawable/ic_auto_svg"
            android:drawableTint="@color/uygulamaMavisi"
            android:paddingLeft="12dp"
            android:text="Sağlık"
            android:textAllCaps="false"
            android:textColor="@color/uygulamaMavisi"
            android:textSize="@dimen/baslarkenEtiketTextSize400"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tarihEtiketButon"
            tools:ignore="MissingConstraints" />

        <Button
            android:id="@+id/oyunEtiketButon"
            android:layout_width="@dimen/baslarkenEtiketButonWidth400"
            android:layout_height="@dimen/baslarkenEtiketButonHeight400"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="8dp"
            android:background="@drawable/baslarkenetiketbuton"
            android:drawableLeft="@drawable/ic_moda_svg"
            android:drawableTint="@color/uygulamaMavisi"
            android:paddingLeft="12dp"
            android:text="Oyun"
            android:textAllCaps="false"
            android:textColor="@color/uygulamaMavisi"
            android:textSize="@dimen/baslarkenEtiketTextSize400"
            app:layout_constraintStart_toEndOf="@+id/saglikEtiketButon"
            app:layout_constraintTop_toBottomOf="@+id/egitimEtiketButon"
            tools:ignore="MissingConstraints" />

        <Button
            android:id="@+id/teknoEtiketButon"
            android:layout_width="@dimen/baslarkenEtiketButonWidth400"
            android:layout_height="@dimen/baslarkenEtiketButonHeight400"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="8dp"
            android:background="@drawable/baslarkenetiketbuton"
            android:drawableLeft="@drawable/ic_moda_svg"
            android:drawableTint="@color/uygulamaMavisi"
            android:paddingLeft="12dp"
            android:text="Oyun"
            android:textAllCaps="false"
            android:textColor="@color/uygulamaMavisi"
            android:textSize="@dimen/baslarkenEtiketTextSize400"
            app:layout_constraintStart_toEndOf="@+id/oyunEtiketButon"
            app:layout_constraintTop_toBottomOf="@+id/muzikEtiketButon"
            tools:ignore="MissingConstraints" />


    </androidx.constraintlayout.widget.ConstraintLayout>

    <TextView
        android:id="@+id/textView9"
        android:layout_width="350dp"
        android:layout_height="38dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="32dp"
        android:text="@string/bildirimAciklamaTxt"
        android:textSize="11sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.48"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/yirmiDortSaatBildirimButon" />


</androidx.constraintlayout.widget.ConstraintLayout>