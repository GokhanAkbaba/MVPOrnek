                            android:layout_width="@dimen/soruPaylasEtiketBtnWidth320"
                            android:layout_height="@dimen/soruPaylasEtiketBtnHeight320"
                            android:layout_marginStart="@dimen/soruPaylasEtiketBtnLayoutMarginStart400"
                            android:layout_marginTop="15dp"
                            android:background="@drawable/soru_etiket_buton_beyaz"
                            android:hint="@string/filmDiziEtiketTxt"
                            android:textColorHint="@color/uygulamaMavisi"
                            android:textSize="@dimen/soruPaylasEtiketBtnTextSize320"
                            app:layout_constraintStart_toEndOf="@+id/tatilEtiketBtn" />

                        <Button
                            android:id="@+id/modaEtiketBtn"
