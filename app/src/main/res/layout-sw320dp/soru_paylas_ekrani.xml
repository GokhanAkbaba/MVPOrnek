                            android:layout_height="@dimen/soruPaylasEtiketBtnHeight320"
                            android:layout_marginTop="15dp"
                            android:background="@drawable/soru_etiket_buton_beyaz"
                            android:clickable="true"
                            android:hint="@string/yemekEtiketTxt"
                            android:textColorHint="@color/uygulamaMavisi"
                            android:textSize="@dimen/soruPaylasEtiketBtnTextSize320"
                            app:layout_constraintStart_toStartOf="parent" />


                        <Button
