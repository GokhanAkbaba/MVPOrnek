<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/anaSayfaLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:ignore="MissingDefaultResource">


    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/anasayfa_nav_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/rounded"
        app:itemIconSize="32dp"
        app:itemIconTint="@color/nav_color"
        app:itemTextColor="@color/nav_color"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:menu="@menu/nav_items" />


    <FrameLayout
        android:id="@+id/anaSayfaFrameLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        tools:ignore="MissingConstraints"
        >


        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/soruPaylasimButon"
            android:layout_width="38dp"
            android:layout_height="38dp"
            android:layout_gravity="bottom|right|center"
            android:layout_marginEnd="15dp"
            android:layout_marginBottom="70dp"
            android:backgroundTint="@color/uygulamaMavisiTwo"
            android:backgroundTintMode="src_in"
            android:clickable="true"
            android:src="@mipmap/soru_gonder_icon"
            android:tint="@color/profilSekmeBeyaz"
            app:backgroundTint="#FFFFFF"
            app:fabCustomSize="38dp"
            app:fabSize="normal"
            tools:ignore="MissingConstraints" />

    </FrameLayout>

</androidx.constraintlayout.widget.ConstraintLayout>