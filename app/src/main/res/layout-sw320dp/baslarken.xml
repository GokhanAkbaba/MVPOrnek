<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout10"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:ignore="MissingDefaultResource">

    <TextView
        android:id="@+id/textView35"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:text="@string/baslarkenTxt"
        android:textColor="@color/uygulamaMavisi"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.516"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView36"
        android:layout_width="300dp"
        android:layout_height="55dp"
        android:layout_marginTop="8dp"
        android:fontFamily="sans-serif-light"
        android:text="@string/baslarkenMetin"
        android:textAlignment="center"
        android:textSize="13sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView35"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView37"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="12dp"
        android:fontFamily="sans-serif-light"
        android:text="@string/yasadiginizIlTxt"
        android:textColor="#000000"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView36" />


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/ilConstraint"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.061"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView37"
        tools:ignore="MissingConstraints">

        <Spinner
            android:id="@+id/ilAutoCompleteTextView"
            android:layout_width="150dp"
            android:layout_height="50dp"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="1dp"
            android:layout_marginBottom="10dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />


    </androidx.constraintlayout.widget.ConstraintLayout>

    <TextView
        android:id="@+id/textView38"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:fontFamily="sans-serif-light"
        android:text="@string/ilgiAlaniTxt"
        android:textColor="#000000"
        android:textSize="18sp"
        android:typeface="normal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ilConstraint" />

    <TextView
        android:id="@+id/textView39"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="364dp"
        android:fontFamily="sans-serif-light"
        android:text="@string/herKonudaBildirimTxt"
        android:textColor="#000000"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/yirmiDortSaatBildirimButon"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/baslarkenilbuton"
        android:drawableLeft="@mipmap/bildirim_icon"
        android:textAllCaps="false"
        android:textColor="@color/uygulamaMavisi"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView39"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/baslarkenSonrakiBtn"
        android:layout_width="141dp"
        android:layout_height="28dp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/backgraoundapp"
        android:text="Sonraki"
        android:textColor="@color/profilSekmeBeyaz"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.53"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/yirmiDortSaatBildirimButon"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/adresEtiketButon"
        android:layout_width="79dp"
        android:layout_height="34dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/baslarkenetiketbuton"
        android:drawableLeft="@mipmap/ic_launcher_adres"
        android:paddingLeft="1dp"
        android:text="Adres"
        android:textSize="7sp"
        android:textAllCaps="false"
        android:textColor="@color/uygulamaMavisi"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView38"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/yemekEtiketButon"
        android:layout_width="79dp"
        android:layout_height="34dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/baslarkenetiketbuton"
        android:drawableLeft="@mipmap/yemek_icon"
        android:paddingLeft="1dp"
        android:text="Yemek"
        android:textAllCaps="false"
        android:textSize="7sp"
        android:textColor="@color/uygulamaMavisi"
        app:layout_constraintStart_toEndOf="@+id/adresEtiketButon"
        app:layout_constraintTop_toBottomOf="@+id/textView38"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/sporEtiketButon"
        android:layout_width="79dp"
        android:layout_height="34dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/baslarkenetiketbuton"
        android:drawableLeft="@mipmap/spor_icon"
        android:paddingLeft="1dp"
        android:text="Spor"
        android:textAllCaps="false"
        android:textSize="7sp"
        android:textColor="@color/uygulamaMavisi"
        app:layout_constraintStart_toEndOf="@+id/yemekEtiketButon"
        app:layout_constraintTop_toBottomOf="@+id/textView38"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/tatilEtiketButon"
        android:layout_width="81dp"
        android:layout_height="34dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/baslarkenetiketbuton"
        android:drawableLeft="@mipmap/tatil_icon"
        android:paddingLeft="1dp"
        android:text="Tatil"
        android:textSize="7sp"
        android:textAllCaps="false"
        android:textColor="@color/uygulamaMavisi"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/adresEtiketButon"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/alisverisEtiketButon"
        android:layout_width="76dp"
        android:layout_height="34dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/baslarkenetiketbuton"
        android:drawableLeft="@mipmap/alisveris_icon"
        android:paddingLeft="1dp"
        android:text="Alışveriş"
        android:textSize="7sp"
        android:textAllCaps="false"
        android:textColor="@color/uygulamaMavisi"
        app:layout_constraintStart_toEndOf="@+id/tatilEtiketButon"
        app:layout_constraintTop_toBottomOf="@+id/yemekEtiketButon"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/sanatEtiketButon"
        android:layout_width="80dp"
        android:layout_height="34dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/baslarkenetiketbuton"
        android:drawableLeft="@mipmap/film_icon"
        android:paddingLeft="1dp"
        android:text="Sanat"
        android:textSize="7sp"
        android:textAllCaps="false"
        android:textColor="@color/uygulamaMavisi"
        app:layout_constraintStart_toEndOf="@+id/alisverisEtiketButon"
        app:layout_constraintTop_toBottomOf="@+id/sporEtiketButon"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="12dp"
        android:textSize="10sp"
        android:text="@string/bildirimAciklamaTxt"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/yirmiDortSaatBildirimButon" />



</androidx.constraintlayout.widget.ConstraintLayout>