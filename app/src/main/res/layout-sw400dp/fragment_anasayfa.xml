                    android:layout_height="@dimen/fragmentAnaSayfaButonHeight400"
                    android:layout_marginLeft="35dp"
                    android:background="@drawable/ana_sayfa_adres_buton"
                    android:src="@mipmap/adres_icon"
                    android:tint="@color/white"
                    app:layout_constraintLeft_toRightOf="@+id/anasayfa_tatil_btn"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:ignore="MissingConstraints" />

                <TextView
                    android:id="@+id/adresAnaSayfaTxt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:text="ADRES"
