                            app:layout_constraintLeft_toLeftOf="@+id/anasayfa_oyun_btn"
                            app:layout_constraintStart_toEndOf="@+id/anasayfa_oyun_btn"
                            app:layout_constraintTop_toTopOf="parent">

                            <ImageView
                                android:id="@+id/anaSayfaSaglikButonIamge"
                                android:layout_width="25dp"
                                android:layout_height="25dp"
                                android:layout_alignParentStart="true"
                                android:layout_alignParentTop="true"
                                android:layout_alignParentEnd="true"
                                android:layout_alignParentBottom="true"
                                android:layout_marginStart="9.5dp"
                                android:layout_marginTop="10dp"
                                android:layout_marginEnd="9.5dp"
                                android:layout_marginBottom="10dp"
                                android:scaleType="centerInside"
                                android:src="@drawable/ic_saglik_svg"
                                android:tint="@color/white"
                                tools:ignore="MissingConstraints" />
                        </RelativeLayout>

                        <TextView
                            android:id="@+id/saglikAnaSayfaTxt"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="8dp"
                            android:text="SAĞLIK"
                            android:textAppearance="@style/customfontstyle"
                            android:textColor="@color/renkSiyah"
                            android:textSize="@dimen/fragmentAnaSayfaButonTextSize400"
                            app:layout_constraintEnd_toEndOf="@+id/anasayfa_saglik_btn"
                            app:layout_constraintStart_toStartOf="@+id/anasayfa_saglik_btn"
                            app:layout_constraintTop_toBottomOf="@+id/anasayfa_saglik_btn" />

                        <RelativeLayout
                            android:id="@+id/anasayfa_muzik_btn"
                            android:layout_width="@dimen/fragmentAnaSayfaButonWidth400"
                            android:layout_height="@dimen/fragmentAnaSayfaButonHeight400"
                            android:layout_marginLeft="35dp"
                            android:layout_marginRight="15dp"
                            android:background="@drawable/ana_sayfa_muzik"
                            app:layout_constraintHorizontal_bias="0.0"
                            app:layout_constraintLeft_toLeftOf="@+id/anasayfa_saglik_btn"
                            app:layout_constraintStart_toEndOf="@+id/anasayfa_saglik_btn"
