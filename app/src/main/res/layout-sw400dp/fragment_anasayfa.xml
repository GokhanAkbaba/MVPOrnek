<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:ignore="MissingDefaultResource">

    <ScrollView
        android:id="@+id/filename_scroll"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="true"
        android:scrollbars="none"
        android:isScrollContainer="false"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints">

<RelativeLayout
    android:id="@+id/anaSayfaLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:ignore="MissingDefaultResource">

    <FrameLayout
        android:id="@+id/anaSayfaMenuFrameLayout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/anaSayfaLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="NotSibling">

        <HorizontalScrollView
            android:id="@+id/anasayfa_horizontalview"
            android:layout_width="match_parent"
            android:layout_height="120dp"
            android:scrollbars="none"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/anasayfa_baslik_tv">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/anaSayfaMenuLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"

                android:layout_marginTop="15dp"
                android:orientation="horizontal">


                <ImageButton
                    android:id="@+id/anasayfa_alisveris_btn"
                    android:layout_width="@dimen/fragmentAnaSayfaButonWidth400"
                    android:layout_height="@dimen/fragmentAnaSayfaButonHeight400"
                    android:background="@drawable/ana_sayfa_alisveris_buton"
                    android:paddingLeft="7dip"
                    android:paddingRight="7dip"
                    android:layout_marginLeft="15dp"
                    android:src="@mipmap/alisveris_icon"
                    android:textSize="8sp"
                    android:tint="@color/white"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/alisverisAnaSayfaTxt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="16dp"
                    android:text="ALIŞVERİŞ"
                    android:textAppearance="@style/customfontstyle"
                    android:textColor="@color/renkSiyah"
                    android:textSize="@dimen/fragmentAnaSayfaButonTextSize400"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="@+id/anasayfa_alisveris_btn"
                    app:layout_constraintStart_toStartOf="@+id/anasayfa_alisveris_btn"
                    app:layout_constraintTop_toBottomOf="@+id/anasayfa_alisveris_btn" />

                <ImageButton
                    android:id="@+id/anasayfa_yemek_btn"
                    android:layout_width="@dimen/fragmentAnaSayfaButonWidth400"
                    android:layout_height="@dimen/fragmentAnaSayfaButonHeight400"
                    android:layout_marginLeft="35dp"
                    android:background="@drawable/ana_sayfa_yemek_buton"
                    android:scaleType="center"
                    android:src="@mipmap/yemek_icon"
                    android:tint="@color/white"
                    app:layout_constraintLeft_toRightOf="@+id/anasayfa_alisveris_btn"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/yemekAnaSayfaTxt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:text="YEMEK"
                    android:textAppearance="@style/customfontstyle"
                    android:textColor="@color/renkSiyah"
                    android:textSize="@dimen/fragmentAnaSayfaButonTextSize400"
                    app:layout_constraintEnd_toEndOf="@+id/anasayfa_yemek_btn"
                    app:layout_constraintStart_toStartOf="@+id/anasayfa_yemek_btn"
                    app:layout_constraintTop_toBottomOf="@+id/anasayfa_yemek_btn" />

                <ImageButton
                    android:id="@+id/anasayfa_tatil_btn"
                    android:layout_width="@dimen/fragmentAnaSayfaButonWidth400"
                    android:layout_height="@dimen/fragmentAnaSayfaButonHeight400"
                    android:layout_marginLeft="35dp"
                    android:background="@drawable/ana_sayfa_tatil_buton"
                    android:scaleType="center"
                    android:src="@mipmap/tatil_icon"
                    android:tint="@color/white"
                    app:layout_constraintLeft_toRightOf="@+id/anasayfa_yemek_btn"
                    app:layout_constraintTop_toTopOf="parent" />


