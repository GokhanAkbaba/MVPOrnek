                            android:textAppearance="@style/customfontstyle"
                            android:textColor="@color/renkSiyah"
                            android:textSize="@dimen/fragmentAnaSayfaButonTextSize400"
                            app:layout_constraintEnd_toEndOf="@+id/anasayfa_oto_btn"
                            app:layout_constraintStart_toStartOf="@+id/anasayfa_oto_btn"
                            app:layout_constraintTop_toBottomOf="@+id/anasayfa_oto_btn" />

                        <RelativeLayout
                            android:id="@+id/anasayfa_yazilim_btn"
                            android:layout_width="@dimen/fragmentAnaSayfaButonWidth400"
                            android:layout_height="@dimen/fragmentAnaSayfaButonHeight400"
                            android:layout_marginLeft="35dp"
                            android:layout_marginRight="15dp"
                            android:background="@drawable/ana_sayfa_yazilim"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="0.0"
                            app:layout_constraintLeft_toLeftOf="@+id/anasayfa_oto_btn"
                            app:layout_constraintStart_toEndOf="@+id/anasayfa_oto_btn"
                            app:layout_constraintTop_toTopOf="parent">

                            <ImageView
                                android:id="@+id/anaSayfaYazilimButonIamge"
                                android:layout_width="25dp"
                                android:layout_height="25dp"
                                android:layout_alignParentStart="true"
                                android:layout_alignParentTop="true"
                                android:layout_alignParentEnd="true"
                                android:layout_alignParentBottom="true"
                                android:layout_marginStart="9.5dp"
                                android:layout_marginTop="10dp"
                                android:layout_marginEnd="9.5dp"
                                android:layout_marginBottom="10dp"
                                android:scaleType="centerInside"
                                android:src="@drawable/ic_engineer"
                                android:tint="@color/white"
                                tools:ignore="MissingConstraints" />
                        </RelativeLayout>

                        <TextView
                            android:id="@+id/yazilimAnaSayfaTxt"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="8dp"
                            android:text="Yazılım"
                            android:textColor="@color/renkSiyah"
                            android:textSize="@dimen/fragmentAnaSayfaButonTextSize400"
                            android:textAppearance="@style/customfontstyle"
                            app:layout_constraintEnd_toEndOf="@+id/anasayfa_yazilim_btn"
                            app:layout_constraintStart_toStartOf="@+id/anasayfa_yazilim_btn"
                            app:layout_constraintTop_toBottomOf="@+id/anasayfa_yazilim_btn" />

            </androidx.constraintlayout.widget.ConstraintLayout>

        </HorizontalScrollView>
    </FrameLayout>

    <androidx.swiperefreshlayout.widget.SwipeRefreshLayout
        android:id="@+id/swiperefreshAnasayfa"
        android:layout_below="@id/anaSayfaMenuFrameLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintTop_toBottomOf="@+id/anaSayfaMenuFrameLayout"
        tools:ignore="MissingConstraints,NotSibling"
        tools:layout_editor_absoluteX="4dp">
        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/recyclerViewSoruAlani"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
           />
        <ProgressBar
            android:id="@+id/progressBarAnaSayfa"
            style="?android:attr/progressBarStyleLarge"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="gone"
             />
    </androidx.swiperefreshlayout.widget.SwipeRefreshLayout>
    </RelativeLayout>
    </ScrollView>

    <include layout="@layout/yorumlar_bottom_sheet" />

    <TextView
        android:id="@+id/recyclerViewSoruAlaniText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="bottom"
        android:text="Gösterilecek bir şey yok"
        android:visibility="invisible"
        android:textAppearance="@style/customfontstyle"
        app:layout_anchor="@+id/filename_scroll"
        app:layout_anchorGravity="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.coordinatorlayout.widget.CoordinatorLayout>