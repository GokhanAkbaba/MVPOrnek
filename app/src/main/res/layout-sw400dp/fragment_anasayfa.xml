                            app:layout_constraintStart_toStartOf="@+id/anasayfa_muzik_btn"
                            app:layout_constraintTop_toBottomOf="@+id/anasayfa_muzik_btn" />

                        <RelativeLayout
                            android:id="@+id/anasayfa_egitim_btn"
                            android:layout_width="@dimen/fragmentAnaSayfaButonWidth400"
