                    android:textAppearance="@style/customfontstyle"
                    android:textSize="@dimen/fragmentAnaSayfaButonTextSize400"
                    app:layout_constraintEnd_toEndOf="@+id/anasayfa_adres_btn"
                    app:layout_constraintStart_toStartOf="@+id/anasayfa_adres_btn"
                    app:layout_constraintTop_toBottomOf="@+id/anasayfa_adres_btn" />

                <ImageButton
                    android:id="@+id/anasayfa_spor_btn"
                    android:layout_width="@dimen/fragmentAnaSayfaButonWidth400"
                    android:layout_height="@dimen/fragmentAnaSayfaButonHeight400"
                    android:layout_marginLeft="35dp"
                    android:background="@drawable/ana_sayfa_spor_buton"
                    android:scaleType="center"
                    android:src="@mipmap/spor_icon"
                    android:tint="@color/white"
                    app:layout_constraintLeft_toRightOf="@+id/anasayfa_adres_btn"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:ignore="MissingConstraints" />

                <TextView
                    android:id="@+id/sporAnaSayfaTxt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
