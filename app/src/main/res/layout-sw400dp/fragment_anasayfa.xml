                        app:layout_constraintEnd_toEndOf="@+id/anasayfa_tekno_btn"
                        app:layout_constraintStart_toStartOf="@+id/anasayfa_tekno_btn"
                        app:layout_constraintTop_toBottomOf="@+id/anasayfa_tekno_btn" />

                        <RelativeLayout
