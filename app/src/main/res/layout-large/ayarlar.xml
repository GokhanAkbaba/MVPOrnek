<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout5"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:ignore="MissingDefaultResource">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <RelativeLayout
            android:id="@+id/searchContentAppbar"
            android:layout_width="match_parent"
            android:layout_height="@dimen/toolbar_size"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints">

            <TextView
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="35dp"
                android:layout_centerVertical="true"
                android:text="@string/ayarlarTxt"
                android:textStyle="bold"
                android:textAppearance="@style/customfontstyle"
                android:textSize="21sp"
                />

        </RelativeLayout>

        <LinearLayout
            android:id="@+id/linearLayout17"
            android:layout_width="match_parent"
            android:layout_height="57dp"
            android:background="@color/colorGrayPrimay"
            android:orientation="vertical"
            android:layout_below="@id/searchContentAppbar">

            <TextView
                android:id="@+id/textView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="15dp"
                android:textAppearance="@style/customfontstyle"
                android:text="@string/ayarlarSayfaHesapTxt"
                android:textSize="20sp" />
        </LinearLayout>

        <RelativeLayout
            android:id="@+id/profilDuzenleLayout"
            android:layout_width="match_parent"
            android:layout_height="@dimen/ayarlarSayfaLayoutHeight400"
            android:background="@drawable/ripple"
            android:clickable="true"
            android:layout_below="@+id/linearLayout17"
            tools:ignore="MissingConstraints">

            <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginStart="15dp"
                android:layout_marginTop="19dp"
                android:textAppearance="@style/customfontstyle"
                android:text="@string/ayarlarSayfaProfilDuzenleTxt"
                android:textSize="@dimen/ayarlarSayfaTextSize400" />

            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/ayarlarSayfaImageViewHeght400"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_marginStart="@dimen/ayarlarSayfaImageViewMarginStart400"
                android:layout_marginTop="@dimen/ayarlarSayfaImageViewMarginTop400"
                android:tint="@color/ayarlarGrisi"
                android:src="@mipmap/search_content_level_one_back_icon" />

            <View
                android:id="@+id/divider2"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginStart="0dp"
                android:layout_marginTop="54dp"
                android:background="@color/ayarlarGrisi"
                app:layout_constraintStart_toStartOf="parent" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/sifreDegistirLayout"
            android:layout_width="match_parent"
            android:layout_height="@dimen/ayarlarSayfaLayoutHeight400"
            android:background="@drawable/ripple"
            android:clickable="true"
            android:layout_below="@+id/profilDuzenleLayout"
            tools:ignore="MissingConstraints">

            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/ayarlarSayfaImageViewHeght400"
                android:layout_alignParentEnd="true"
                android:layout_marginStart="@dimen/ayarlarSayfaImageViewMarginStart400"
                android:layout_marginTop="@dimen/ayarlarSayfaImageViewMarginTop400"
                android:src="@mipmap/search_content_level_one_back_icon"
                android:tint="@color/ayarlarGrisi" />

            <TextView
                android:id="@+id/textView13"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginStart="14dp"
                android:layout_marginTop="19dp"
                android:textAppearance="@style/customfontstyle"
                android:text="@string/ayarlarSayfaSifreDegistir"
                android:textSize="@dimen/ayarlarSayfaTextSize400" />

            <View
                android:id="@+id/divider3"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="0dp"
                android:layout_marginTop="54dp"
                android:background="@color/ayarlarGrisi" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/bildirimlerLayout"
            android:layout_width="match_parent"
            android:layout_height="@dimen/ayarlarSayfaLayoutHeight400"
            android:background="@drawable/ripple"
            android:clickable="true"
            android:layout_below="@+id/sifreDegistirLayout"
            tools:ignore="MissingConstraints">

            <ImageView
                android:id="@+id/imageView7"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/ayarlarSayfaImageViewHeght400"
                android:layout_alignParentEnd="true"
                android:layout_marginStart="@dimen/ayarlarSayfaImageViewMarginStart400"
                android:layout_marginTop="@dimen/ayarlarSayfaImageViewMarginTop400"
                android:src="@mipmap/search_content_level_one_back_icon"
                android:tint="@color/ayarlarGrisi" />

            <TextView
                android:id="@+id/textView15"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginStart="15dp"
                android:layout_marginTop="19dp"
                android:text="@string/ayarlarSayfaBildirimler"
                android:textAppearance="@style/customfontstyle"
                android:textSize="@dimen/ayarlarSayfaTextSize400" />
            <View
                android:id="@+id/divider4"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="0dp"
                android:layout_marginTop="54dp"
                android:background="@color/ayarlarGrisi" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/cikisYapLayout"
            android:layout_width="match_parent"
            android:layout_height="@dimen/ayarlarSayfaLayoutHeight400"
            android:background="@drawable/ripple"
            android:layout_below="@+id/bildirimlerLayout"
            android:clickable="true"
            tools:ignore="MissingConstraints">

            <ImageView
                android:id="@+id/imageView8"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/ayarlarSayfaImageViewHeght400"
                android:layout_alignParentEnd="true"
                android:layout_marginStart="@dimen/ayarlarSayfaImageViewMarginStart400"
                android:layout_marginTop="@dimen/ayarlarSayfaImageViewMarginTop400"
                android:src="@mipmap/search_content_level_one_back_icon"
                android:tint="@color/ayarlarGrisi" />

            <TextView
                android:id="@+id/textView44"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginStart="15dp"
                android:layout_marginTop="19dp"
                android:textAppearance="@style/customfontstyle"
                android:text="@string/ayarlarSayfaCikisYap"
                android:textSize="@dimen/ayarlarSayfaTextSize400"/>



        </RelativeLayout>
    </RelativeLayout>

</androidx.constraintlayout.widget.ConstraintLayout>