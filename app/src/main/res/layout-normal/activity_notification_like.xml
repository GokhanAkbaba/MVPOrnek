<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activity.Notification.NotificationLikeActivity"
    tools:ignore="MissingDefaultResource">
    <RelativeLayout
        android:id="@+id/notificationLikeRelativeLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    <androidx.cardview.widget.CardView
        android:id="@+id/notificationLikeCardView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/notificationLikeSorularIcerikLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            tools:layout_editor_absoluteX="1dp"
            tools:layout_editor_absoluteY="1dp">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
            <androidx.cardview.widget.CardView
                android:id="@+id/notificationLikeKullaniciResmi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="6dp"
                app:cardCornerRadius="15dp"
                app:cardElevation="0dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.103">

                <ImageView
                    android:id="@+id/notificationLikeSorularIcerikKullaniciResmi"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:background="@color/white"
                    android:scaleType="centerCrop"
                    android:src="@drawable/rumeli" />
            </androidx.cardview.widget.CardView>

            <TextView
                android:id="@+id/notificationLikeKullaniciAdSoyad"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="10dp"
                android:layout_toEndOf="@+id/notificationLikeKullaniciResmi"
                android:layout_marginTop="10dp"
                android:text="GÃ¶khan AKBABA"
                android:textAppearance="@style/customfontstyle"
                android:textColor="#000"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/notificationLikeSoruPaylasmaZamani"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:textAppearance="@style/customfontstyle"
                android:text="1sa"
                android:textSize="11sp"
                android:layout_below="@+id/notificationLikeKullaniciAdiTxt"
                android:layout_toEndOf="@+id/notificationLikeKullaniciResmi"
               />

            <TextView
                android:id="@+id/notificationLikeSoruEtiketleri"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:text="#Adres #Tatil"
                android:textAppearance="@style/customfontstyle"
                android:textSize="14sp"
                android:layout_below="@+id/notificationLikeSoruPaylasmaZamani"/>

            <TextView
                android:id="@+id/notificationLikeKullaniciAdiTxt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:text="\@GokhanAkbaba"
                android:layout_toEndOf="@+id/notificationLikeKullaniciResmi"
                android:layout_below="@+id/notificationLikeKullaniciAdSoyad"
                android:textAppearance="@style/customfontstyle"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/notificationLikeSoruTxt"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:layout_marginStart="16dp"
                android:layout_marginEnd="10dp"
                android:textAppearance="@style/customfontstyle"
                android:text="@string/denemeSoru1Txt"
                android:textColor="#000"
                android:textSize="17sp"
                android:layout_below="@+id/notificationLikeSoruEtiketleri"
                />

                <RelativeLayout
                    android:id="@+id/relativeLayout"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="15dp"
                    android:layout_marginTop="5dp"
                    android:layout_below="@+id/notificationLikeSoruTxt">
                    <ImageView
                        android:id="@+id/notificationLikeSoruYorumIcon"
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:src="@drawable/ic_comment_svg" />

                    <TextView
                        android:id="@+id/notificationLikeYorumSayiTxt"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="@style/customfontstyle"
                        android:text="155"
                        android:layout_marginTop="-3dp"
                        android:layout_marginStart="4dp"
                        android:layout_toEndOf="@+id/notificationLikeSoruYorumIcon"
                        android:textSize="17sp" />
                </RelativeLayout>

            <View
                android:id="@+id/divider3"
                android:layout_width="453dp"
                android:layout_height="1dp"
                android:layout_marginTop="13.6dp"
                android:background="@color/ayarlarGrisi"
                android:layout_below="@+id/relativeLayout"
                />
            </RelativeLayout>
        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/notificationLikeYorumlarIcerikLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:layout_below="@+id/notificationLikeCardView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.064"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            tools:ignore="MissingConstraints">
        <androidx.cardview.widget.CardView
            android:id="@+id/notificationLikeYorumKullaniciResmi"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="6dp"
            app:cardCornerRadius="15dp"
            app:cardElevation="0dp">

            <ImageView
                android:id="@+id/notificationYorumRoundedKullaniciResmi"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:background="@color/white"
                android:scaleType="centerCrop"
                android:src="@drawable/rumeli"
                tools:ignore="MissingConstraints" />
        </androidx.cardview.widget.CardView>

        <TextView
            android:id="@+id/notificationLikeYorumKullaniciAdi"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="50dp"
            android:text="TextView"
            android:textAppearance="@style/customfontstyle"
            android:textColor="@color/renkSiyah"
            android:textSize="12sp"
            android:layout_toEndOf="@+id/notificationLikeYorumKullaniciResmi"
           />

        <TextView
            android:id="@+id/notificationLikeYorumTxt"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginEnd="50dp"
            android:layout_marginStart="10dp"
            android:text="TextView"
            android:textAppearance="@style/customfontstyle"
            android:textColor="@color/renkSiyah"
            android:textSize="16sp"
            android:layout_below="@+id/notificationLikeYorumKullaniciAdi"
            android:layout_toEndOf="@+id/notificationLikeYorumKullaniciResmi"

            />
            <RelativeLayout
                android:id="@+id/relativeLayout5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="-40dp"
                android:layout_toEndOf="@+id/notificationLikeYorumKullaniciAdi"
                tools:ignore="MissingConstraints">
                <ImageView
                    android:id="@+id/notificationLikeYorumBegeniButon"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:clickable="true"
                    android:src="@mipmap/guven_oyu"
                    android:tint="@color/ayarlarGrisi" />

                <TextView
                    android:id="@+id/notificationLikeYorumBegeniSayisiTxt"
                    android:layout_width="wrap_content"
                    android:layout_height="16dp"
                    android:layout_below="@+id/notificationLikeYorumBegeniButon"
                    android:text="1"
                    android:textAlignment="center"
                    android:layout_centerHorizontal="true"
                    android:textAppearance="@style/customfontstyle" />
            </RelativeLayout>
        <TextView
            android:id="@+id/notificationLikeYorumZaman"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:layout_marginBottom="8dp"
            android:text="TextView"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="10dp"
            android:layout_below="@+id/relativeLayout5"
            android:textAppearance="@style/customfontstyle"
           />

        <View
            android:id="@+id/divider11"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginTop="4dp"
            android:background="?android:attr/listDivider"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />
        </RelativeLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>
    </RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>