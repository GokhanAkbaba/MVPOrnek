                            android:textColor="@color/uygulamaMavisi"
                            android:textSize="13sp" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/tatilEtiketButon"
                        android:layout_width="@dimen/baslarkenSayfaLayoutWidth"
                        android:layout_height="@dimen/baslarkenSayfaLayoutHeight"
                        android:layout_below="@+id/adresEtiketButon"
                        android:layout_marginTop="@dimen/baslarkenSayfaLayoutTop"
                        android:layout_marginEnd="@dimen/baslarkenSayfaLayoutLeft"
                        android:background="@drawable/baslarkenetiketbuton">

                        <ImageView
