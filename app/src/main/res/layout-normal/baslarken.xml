                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/yazilimEtiketButon"
                        android:layout_width="@dimen/baslarkenSayfaLayoutWidth"
                        android:layout_height="@dimen/baslarkenSayfaLayoutHeight"
                        android:layout_below="@+id/tatilEtiketButon"
                        android:layout_marginTop="@dimen/baslarkenSayfaLayoutTop"
                        android:layout_marginEnd="@dimen/baslarkenSayfaLayoutLeft"
                        android:background="@drawable/baslarkenetiketbuton">

                        <ImageView
                            android:id="@+id/yazilimImage"
                            android:layout_width="40dp"
                            android:layout_height="40dp"
                            android:layout_alignParentStart="true"
                            android:layout_alignParentTop="true"
                            android:layout_alignParentBottom="true"
                            android:layout_marginStart="10dp"
                            android:layout_marginTop="1dp"
                            android:layout_marginEnd="10dp"
