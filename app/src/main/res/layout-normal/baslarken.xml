                android:textAppearance="@style/customfontstyle"
                android:textColor="#000000"
                android:textSize="18sp"
                android:typeface="normal"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
